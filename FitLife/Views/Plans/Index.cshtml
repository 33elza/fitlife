@model IEnumerable<FitLife.Models.DBModels.Plan>

@{
    ViewBag.Title = "Index";
}

<h2>Следуемые планы</h2>

<table class="table">

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.ActionLink(@Html.DisplayFor(modelItem => item.Name).ToString(), "Details", new { id = item.ID })
        </td>

    </tr>
}

</table>
